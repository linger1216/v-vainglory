        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vainglory.mapper.PostMapper">

    <resultMap id="BaseResultMap" type="com.vainglory.domain.Post">
            <id property="id" column="id" />
